<div class="container-fluid bg-purple py-2 px-5 move-front">
  <div class="row">
    <nav class="navbar navbar-expand-lg flex-fill">
      <a class="navbar-brand" href="/contact-us">
        <h3 class="text-peach">FastForm</h3>
      </a>
      <ul class="navbar-nav mr-auto">
        <span class="text-peach pt-1">
          <h4>| Help Center</h4>
        </span>
      </ul>
      <div class="dropdown">
        <button type="button" class="btn btn-outline-light border-0 fas fa-user-circle m-2 py-2" type="button"
          id="accountDrop" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>
        <div class="dropdown-menu dropdown-menu-right bg-dark-purple" aria-labelledby="accountDrop">
          <% if(locals.loggedIn && loggedIn) { %>
          <a class="dropdown-item text-cusWhite" href="/user">Profile</a>
          <a class="dropdown-item disabled" href="/settings">Account Settings</a>
          <a class="dropdown-item text-cusWhite" href="/applications">Applications</a>
          <div class="dropdown-divider"></div>
          <a class="dropdown-item text-danger" href="/log-out"><span class="fas fa-sign-out-alt mr-2"></span> Log
            Out</a>
          <% } else { %>
          <a class="dropdown-item text-cusWhite" href="/login">Login</a>
          <a class="dropdown-item disabled" href="/register">Register</a>
          <% } %>
        </div>
      </div>
  </div>
  </nav>
</div>
</div>